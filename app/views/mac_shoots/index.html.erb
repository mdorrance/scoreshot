<h1>Listing mac_shoots</h1>

<table>
  <tr>
    <th>User</th>
    <th>Division</th>
    <th>Score actual</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mac_shoots.each do |mac_shoot| %>
  <tr>
    <td><%= mac_shoot.user_id %></td>
    <td><%= mac_shoot.division %></td>
    <td><%= mac_shoot.score_actual %></td>
    <td><%= mac_shoot.location_id %></td>
    <td><%= link_to 'Show', mac_shoot %></td>
    <td><%= link_to 'Edit', edit_mac_shoot_path(mac_shoot) %></td>
    <td><%= link_to 'Destroy', mac_shoot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mac shoot', new_mac_shoot_path %>
